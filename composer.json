{
    "name": "mekras/atompub",
    "description": "Atom Publishing Protocol support library",
    "license": "MIT",
    "keywords": [
        "atompub",
        "atom"
    ],
    "authors": [
        {
            "name": "Михаил Красильников",
            "email": "m.krasilnikov@yandex.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "mekras/atom": "dev-master",
        "mekras/class-helpers": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0.9",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Mekras\\AtomPub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mekras\\AtomPub\\Tests\\": "tests/"
        }
    }
}
